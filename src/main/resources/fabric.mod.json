{
	"schemaVersion": 1,
	"id": "wikwriv",
	"version": "${version}",
	"name": "Wicks & Wrivets",
	"description": "This is an example description! Tell everyone what your mod is about!",
	"authors": [
		"Zarkanaseen","HollowedWanderer", "Xyndra"
	],
	"contact": {
		"homepage": "https://fabricmc.net/",
		"sources": "https://github.com/FabricMC/fabric-example-mod"
	},
	"license": "CC0-1.0",
	"icon": "assets/wikwriv/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			{
				"adapter": "kotlin",
				"value": "net.hollowed.wikwriv.WicksWrivets"
			}
		],
		"client": [
			{
				"adapter": "kotlin",
				"value": "net.hollowed.wikwriv.client.EntitiesClient"
			}
		],
		"fabric-datagen": [
			{
				"adapter": "kotlin",
				"value": "net.hollowed.wikwriv.WicksWrivetsDataGenerator"
			}
		]
	},
	"mixins": [
		"wikwriv.mixins.json"
	],
	"depends": {
		"fabric-language-kotlin": ">=1.12.3+kotlin.2.0.21",
		"fabricloader": ">=0.16.7",
		"minecraft": "~1.21.1",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}